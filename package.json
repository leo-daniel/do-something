    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "concurrently": "^4.0.1",
    "express": "^4.16.3",
    "if-env": "^1.0.4",
    "mongoose": "^5.2.16",
    "nodemon": "^1.18.4"
  }
}
